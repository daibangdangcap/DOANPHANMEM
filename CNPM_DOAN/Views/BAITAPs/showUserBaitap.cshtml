@model IEnumerable<CNPM_DOAN.Models.NGUOIDUNG>

@{
    ViewBag.Title = "showUserBaiTap";
}

<link rel="stylesheet" href="~/MyCss/nhomhoctap.css" />
<div class="content">
    <h1>@CNPM_DOAN.Resources.Language.Danh_sách_học_sinh</h1>
    <div class="content-group">
        @foreach (var item in Model)
        {
            <div class="content-group-item">
                <div class="content-group-item-name" onclick="location.href='@Url.Action("showBaiTap_PH", "BAITAPs", new {iduser=item.IDNguoiDung})'">
                    <p>@item.TenNguoiDung</p>
                </div>
            </div>
        }
    </div>
</div>
